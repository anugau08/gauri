class Employee 
{ 
String fname; 
String Iname; 
int id; 
Employee(String fname, String Iname, int id) //Constructor {this.fname=fname; //"this" represents the current obj this.Iname=lname; 
this.id=id; 
} 
// Public String toString() 
{ 
//return fname+" "+lname+" "+id; 
} 
public int hashCode() 
{ 
return id; 
} 
} 
class Test 
{public static void main(String[] args) 
{Employee e1=new Employee("Peter", "Parkar",101); 
Employee e2=new Employee("Peter", "Parkar",101); 
Employee e3=e1; //e3 is reference variable of type employee System.out.println(e1); 
System.out.println(e2); 
System.out.println(e1.equals(e2)); 
System.out.println(e1.equals(e3)); 
} 
} 
